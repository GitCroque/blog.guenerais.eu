{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "Publishing date",
          "name": "date",
          "type": "datetime",
          "default": "{{now}}",
          "isPublishDate": true
        },
        {
          "title": "Content preview",
          "name": "preview",
          "type": "image"
        },
        {
          "title": "Is in draft",
          "name": "draft",
          "type": "draft"
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "Categories",
          "name": "categories",
          "type": "categories"
        }
      ]
    }
  ],
  "frontMatter.framework.id": "hugo",
  "frontMatter.content.publicFolder": "static",
  "frontMatter.preview.host": "http://localhost:1313",
  "frontMatter.content.pageFolders": [
    {
      "path": "[[workspace]]/archetypes",
      "title": "archetypes"
    },
    {
      "path": "[[workspace]]/themes/hugo-theme-stack"
    },
    {
      "path": "[[workspace]]/themes/hugo-theme-stack/exampleSite"
    },
    {
      "path": "[[workspace]]/themes/hugo-theme-stack/archetypes"
    },
    {
      "path": "[[workspace]]/themes/hugo-theme-stack/exampleSite/content"
    },
    {
      "path": "[[workspace]]/themes/hugo-theme-stack/exampleSite/content/categories/Test"
    },
    {
      "path": "[[workspace]]/themes/hugo-theme-stack/exampleSite/content/post/rich-content"
    },
    {
      "path": "[[workspace]]/themes/hugo-theme-stack/exampleSite/content/page/search"
    },
    {
      "path": "[[workspace]]/themes/hugo-theme-stack/exampleSite/content/post/placeholder-text"
    },
    {
      "path": "[[workspace]]/themes/hugo-theme-stack/exampleSite/content/page/archives"
    },
    {
      "path": "[[workspace]]/themes/hugo-theme-stack/exampleSite/content/post/math-typesetting"
    },
    {
      "path": "[[workspace]]/themes/hugo-theme-stack/exampleSite/content/page/links"
    },
    {
      "path": "[[workspace]]/themes/hugo-theme-stack/exampleSite/content/post/markdown-syntax"
    },
    {
      "path": "[[workspace]]/themes/hugo-theme-stack/exampleSite/content/page/about"
    },
    {
      "path": "[[workspace]]/themes/hugo-theme-stack/exampleSite/content/post/emoji-support"
    },
    {
      "path": "[[workspace]]/themes/hugo-theme-stack/exampleSite/content/post/chinese-test"
    }
  ],
  "frontMatter.git.enabled": true
}